{
	"name": "@vdegenne/forms",
	"version": "0.0.8",
	"type": "module",
	"scripts": {
		"build": "wireit"
	},
	"wireit": {
		"build": {
			"command": "tsc --pretty",
			"clean": "if-file-deleted",

				"./tsconfig.json",
				"./src/"
			],
			"output": [
				"./lib/"
			]
		}
	},
	"devDependencies": {
		"@material/web": "^2.2.0",
		"lit": "^3.2.1",
		"typescript": "^5.7.3",
		"wireit": "^0.14.11"
	},
	"files": [
		"./lib/"
	],
	"exports": {
		".": {
			"default": "./lib/index.js",
			"types": "./lib/index.d.ts"
		}
	}
}

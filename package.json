{
	"name": "@vdegenne/forms",
	"version": "0.1.63",
	"type": "module",
	"scripts": {
		"build": "wireit"
	},
	"wireit": {
		"build": {
			"command": "tsc --pretty",
			"clean": "if-file-deleted",
			"files": [
				"./tsconfig.json",
				"./src/"
			],
			"output": [
				"./lib/"
			]
		}
	},
	"devDependencies": {
		"@material/web": "^2.4.1",
		"lit": "^3.3.1",
		"typescript": "^5.9.3",
		"wireit": "^0.14.12"
	},
	"dependencies": {
		"@vdegenne/debouncer": "^1.0.9"
	},
	"peerDependencies": {
		"@material/web": "^2.0.0",
		"lit": "^3.0.0"
	},
	"files": [
		"./lib/"
	],
	"exports": {
		".": {
			"import": "./lib/index.js",
			"types": "./lib/index.d.ts"
		},
		"./FormBuilder.js": {
			"import": "./lib/FormBuilder.js",
			"types": "./lib/FormBuilder.d.ts"
		},
		"./bindInput.js": {
			"import": "./lib/bindInput.js",
			"types": "./lib/bindInput.d.ts"
		}
	}
}
